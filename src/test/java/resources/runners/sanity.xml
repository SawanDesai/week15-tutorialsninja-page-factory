<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sanity Suite" verbose="1">
    <groups>
        <run>
            <include name="sanity"></include>

        </run>
    </groups>
    <listeners>
        <listener class-name="demo.com.tutorialsninja.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Sanity Test">
        <classes>
            <class name="demo.com.tutorialsninja.testsuite.DesktopTest"/>
            <class name="demo.com.tutorialsninja.testsuite.HomePageTest"/>
            <class name="demo.com.tutorialsninja.testsuite.LaptopAndNoteBooksTest"/>
            <class name="demo.com.tutorialsninja.testsuite.MyAccountTest"/>
        </classes>
    </test>
</suite>
